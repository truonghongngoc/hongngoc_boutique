{
  "name": "trading-planning-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build --mode production",
    "tsc": "tsc",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.tsx --fix && prettier \"src/**/*.{js,ts,tsx}\" --write",
    "preview": "vite preview",
    "generate-api": "find ./openapi -type f | xargs rm -f && export $(cat .env | grep -v ^# | xargs) && npx swagger-typescript-api --axios -p ./api-doc/openapi.yml -o ./openapi/ --extract-request-params --modular",
    "check": "npm run tsc && npm run lint:fix"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.9.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.202",
    "@types/qs": "^6.9.10",
    "@types/react-datepicker": "^4.19.3",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.6.0",
    "framer-motion": "^10.16.4",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "numeral": "^2.0.6",
    "prettier": "^3.0.3",
    "qs": "^6.11.2",
    "react": "^18.3.1",
    "react-datepicker": "^4.23.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.1",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.18.0",
    "react-select": "^5.8.0",
    "swagger-typescript-api": "^13.0.3",
    "use-debounce": "^10.0.0",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@types/numeral": "^2.0.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-svgr": "^4.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  }
}
